<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OutlineTextView">
        <attr name="outlineSize" format="dimension" />
        <attr name="outlineColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="NGKaryaToolbar">
        <attr name="backgroundTint" format="color|reference" />
        <attr name="cutoutMargin" format="dimension" />
        <attr name="cutoutRoundedCornerRadius" format="dimension" />
        <attr name="cutoutVerticalOffset" format="dimension" />
        <attr name="elevation" format="dimension" />
    </declare-styleable>

    <declare-styleable name="GenderCard">
        <attr name="title" format="string|reference" />
        <attr name="icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="NumPad">
        <attr name="field" format="reference" />
        <attr name="maxLength" format="integer" />
    </declare-styleable>
</resources>